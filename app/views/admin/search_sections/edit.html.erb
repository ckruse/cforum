<%
content_for :title, t('admin.search_sections.edit', name: @search_section.name)
content_for :h1, t('admin.search_sections.edit', name: @search_section.name)
%>

<%= form_for @search_section, url: admin_search_section_path(@search_section), html: {class: 'cf-form'} do |f| %>

  <%= render 'form', f: f %>

  <p class="form-actions">
    <%= f.submit t('global.save'), class: 'cf-primary-btn' %>
    <%= cf_link_to t('global.cancel'), admin_search_sections_path, class: 'cf-btn' %>
  </p>

<% end %>
